    void InOrderTraverse(TreeNode* node){
        if(!node){
            return;
        }
        
        TreeNode *p;
        p=node;
        
        while(!saveNode.empty()||p){
            while(p){
                saveNode.push(p);
                p=p->left;
            }
            
            p=saveNode.top();
            saveVal.push_back(p->val);
            saveNode.pop();
            
            p=p->right;
        }
    }
