#include <vector>
using namespace std;
打发第三方说的方法稍等
class Solution {
public:
    vector<vector<int>> generate(int numRows) {
        vector<vector<int>> result;
        vector<int> last,current;
        result.clear();
        last.clear();
        current.clear();
        
        if(numRows==0){
            return result;
        }
          
        if(numRows==1){
            last.push_back(1);
            result.push_back(last);
            return result;
        }
        
        current.push_back(1);
        last.push_back(1);
        last.push_back(1);
        result.push_back(current);
        result.push_back(last);
        
        if(numRows==2){
            return result;
        }
        
        for(int i=3;i<=numRows;i++){
            current.clear();
            
            current.push_back(1);
            for(int j=0;j<i-2;j++){
                int m=last[j]+last[j+1];
                current.push_back(m);
            }
            current.push_back(1);
            
            result.push_back(current);
            
            last.clear();
            last.assign(current.begin(),current.end());
        }
        
        return result;
    }
};
